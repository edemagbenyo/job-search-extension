{
  "name": "Remotic ",
  "description": "Get remote jobs without the need to browser many sites.",
  "version": "1.0.2",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.ts"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "declarativeContent",
    "http://localhost:5001/",
    "https://us-central1-job-ext.cloudfunctions.net/"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens job board"
    }
  }
}